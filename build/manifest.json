{
  "manifest_version": 2,
  "name": "one_box",
  "version": "0.0.1",
  "description": "A Chrome Extension",
  "icons": {
    "16": "icons/icon.png",
    "32": "icons/icon.png",
    "48": "icons/icon.png",
    "128": "icons/icon.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_title": "one_box",
    "default_popup": "popup.html"
  },
  "permissions": [
    "*://*/*"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_start",
      "js": [
        "content.js"
      ],
      "css": [
        "content.css"
      ]
    }
  ]
}